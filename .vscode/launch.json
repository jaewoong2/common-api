{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NestJS (Debug)",
      "runtimeExecutable": "npm",
      // NODE_ENV = local
      "env": {
        "NODE_ENV": "local"
      },
      "runtimeArgs": ["run", "start:debug", "--watch"],
      "ports": 9229,
      "autoAttachChildProcesses": true,
      "restart": true,
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
